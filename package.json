{
   "name": "sequelize-ts-cli",
   "version": "1.0.0",
   "description": "Sequelize Typescript Cli",
   "main": "/dist/main.js",
   "repository": "https://github.com/yonycalsin/sequelize-ts-cli.git",
   "author": "Yony Calsin",
   "license": "MIT",
   "private": false,
   "scripts": {
      "format": "prettier --write ./**/*.{ts,js,json}",
      "dev": "tsnd --respawn -T -r tsconfig-paths/register src/main.ts",
      "start": "ts-node -T -r tsconfig-paths/register src/main.ts",
      "build": "run-s clean build:ts",
      "build:ts": "tsc",
      "clean": "rimraf -r dist config models seeders migrations scripts",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "type-check": "tsc --noEmit",
      "fetch": "sh ./scripts/git-fetch.sh"
   },
   "dependencies": {
      "deasync": "^0.1.21",
      "debug": "^4.3.1",
      "tsconfig-paths": "^3.9.0",
      "umzug": "^2.3.0",
      "yargs": "^16.2.0"
   },
   "peerDependencies": {
      "@types/bluebird": "^3.5.33",
      "@types/node": "^14.14.20",
      "@types/validator": "^13.1.2",
      "reflect-metadata": "^0.1.13",
      "sequelize": "^6.3.5",
      "sequelize-typescript": "^1.1.0",
      "ts-node": "^9.0.0",
      "typescript": "^4.0.3"
   },
   "devDependencies": {
      "@types/bluebird": "^3.5.33",
      "@types/deasync": "^0.1.1",
      "@types/debug": "^4.1.5",
      "@types/node": "^14.14.20",
      "@types/umzug": "^2.3.0",
      "@types/validator": "^13.1.2",
      "@types/yargs": "^15.0.12",
      "@typescript-eslint/eslint-plugin": "^4.12.0",
      "@typescript-eslint/parser": "^4.12.0",
      "eslint": "^7.17.0",
      "eslint-config-prettier": "^7.1.0",
      "eslint-plugin-import": "^2.22.1",
      "husky": "^5.0.9",
      "lint-staged": "^10.5.3",
      "mysql2": "^2.2.5",
      "npm-run-all": "^4.1.5",
      "reflect-metadata": "^0.1.13",
      "sequelize": "5.21.10",
      "sequelize-typescript": "^1.1.0",
      "ts-node": "^9.0.0",
      "ts-node-dev": "^1.0.0",
      "typescript": "^4.0.3"
   },
   "bin": {
      "sts": "./dist/main",
      "sequelize-ts-cli": "./dist/main",
      "stc": "./dist/main"
   }
}

{
   "name": "sequelize-ts-cli",
   "version": "1.0.0",
   "description": "Sequelize Typescript Cli",
   "main": "/dist/main.js",
   "repository": "https://github.com/yonycalsin/sequelize-ts-cli.git",
   "author": "Yony Calsin",
   "license": "MIT",
   "private": false,
   "scripts": {
      "format": "prettier --write ./**/*.{ts,js,json}",
      "dev": "tsnd --respawn -T -r tsconfig-paths/register src/main.ts",
      "start": "ts-node -T -r tsconfig-paths/register src/main.ts",
      "build": "run-s clean build:ts",
      "build:ts": "tsc",
      "clean": "rimraf -r dist config models seeders migrations scripts",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "type-check": "tsc --noEmit"
   },
   "dependencies": {
      "tsconfig-paths": "^3.9.0"
   },
   "peerDependencies": {
      "sequelize-cli": "^6.2.0",
      "ts-node": "^9.0.0",
      "typescript": "^4.0.3"
   },
   "devDependencies": {
      "@typescript-eslint/eslint-plugin": "^4.12.0",
      "@typescript-eslint/parser": "^4.12.0",
      "eslint": "^7.17.0",
      "eslint-config-prettier": "^7.1.0",
      "eslint-plugin-import": "^2.22.1",
      "husky": "^4.3.7",
      "lint-staged": "^10.5.3",
      "npm-run-all": "^4.1.5",
      "sequelize": "^6.3.5",
      "sequelize-cli": "^6.2.0",
      "ts-node": "^9.0.0",
      "ts-node-dev": "^1.0.0",
      "typescript": "^4.0.3"
   },
   "bin": {
      "sts": "./dist/main",
      "sequelize-ts-cli": "./dist/main",
      "stc": "./dist/main"
   }
}
